    "def printWordsKFreq(s,k):\n",
    "    words=s.split()\n",
    "    d={}\n",
    "    for w in words:\n",
    "        d[w]=d.get(w,0)+1\n",
    "    for i in d:\n",
    "        if d[i]==k:\n",
    "            print(i)\n",
    "\n",
    "s = \"a a a a man is is hello beli beli hello very much much\"\n",
    "printWordsKFreq(s,2)"
   ]
  }
