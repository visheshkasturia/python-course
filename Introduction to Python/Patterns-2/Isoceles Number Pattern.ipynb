    "        print(p,end=\"\")\n",
    "        p+=1\n",
    "    \n",
    "    p=i-1\n",
    "    while p>0:\n",
    "        print(p,end=\"\")\n",
    "        p-=1\n",
    "    \n",
    "    print()\n",
    "    i+=1"
