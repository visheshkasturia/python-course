    "def checkSort(li,si):\n",
    "    l = len(li)\n",
    "    if si==l-1 or si==l:\n",
    "        return True\n",
    "    \n",
    "    if li[si]> li[si+1]:\n",
    "        return False\n",
    "    return checkSort(li,si+1)\n",
    "\n",
    "li=[1,2,3,4,5,6]\n",
    "checkSort(li,0)\n",
    "    "
   ]
  }
