    "def sumArray(li,si):\n",
    "    l=len(li)\n",
    "    if si==l-1:\n",
    "        return li[si]\n",
    "    else:\n",
    "        return li[si] + sumArray(li,si+1)\n",
    "\n",
    "from sys import setrecursionlimit\n",
    "setrecursionlimit(11000)\n",
    "n=int(input())\n",
    "arr=list(int(i) for i in input().strip().split(' '))\n",
    "print(sumArray(arr,0))"
   ]
  }
