    "def power(x,n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    else:\n",
    "        return x*power(x,n-1)\n",
    "\n",
    "s=input().split()\n",
    "x,n=int(s[0]),int(s[1])\n",
    "c=power(x,n)\n",
    "print(c)"
