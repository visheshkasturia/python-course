   "source": [
    "def replaceChar(s,ch1,ch2):\n",
    "    if len(s)==0:\n",
    "        return s\n",
    "    \n",
    "    if s[0]==ch1:\n",
    "        return ch2 + replaceChar(s[1:],ch1,ch2)\n",
    "    \n",
    "    return s[0]+ replaceChar(s[1:],ch1,ch2)\n",
    "\n",
    "s=\"abbababababababbbabab\"\n",
    "print(replaceChar(s,'a','c'))"
   ]
  }
