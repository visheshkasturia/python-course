    "def replacePi(s):\n",
    "    if len(s)==0 or len(s)==1:\n",
    "        return s\n",
    "    if s[0]==\"p\" and s[1]==\"i\":\n",
    "        smallOutput=\"3.14\"+replacePi(s[2:])\n",
    "        return smallOutput\n",
    "    else:\n",
    "        smallOutput=replacePi(s[1:])\n",
    "        return s[0]+smallOutput\n",
    "\n",
    "s=\"aksdpi3.14pi\"\n",
    "print(replacePi(s))"
   ]
  }
