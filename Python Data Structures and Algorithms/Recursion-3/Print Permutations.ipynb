    "def printPermutations(s,output):\n",
    "    if len(s)==0:\n",
    "        print(output)\n",
    "        return\n",
    "    \n",
    "    for i in range(len(s)):\n",
    "        printPermutations(s[0:i]+s[i+1:],output+s[i])\n",
    "\n",
    "printPermutations(\"abc\",\"\")\n",
    "    \n",
    "    \n",
    "    "
   ]
  }
