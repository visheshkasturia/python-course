    "def multi(a,b):\n",
    "    if b==0:\n",
    "        return 0\n",
    "    elif b>0:\n",
    "        return a + multi(a,b-1)\n",
    "    else:\n",
    "        return -a + multi(a,b+1) \n",
    "import sys\n",
    "sys.setrecursionlimit(3000)\n",
    "print(multi(100,1000))"
   ]
  }
