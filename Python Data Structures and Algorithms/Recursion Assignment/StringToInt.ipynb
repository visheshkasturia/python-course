    "def stringtoint(s):\n",
    "    if len(s)==0:\n",
    "        return 0\n",
    "    return int(s[-1]) + 10*stringtoint(s[:len(s)-1])\n",
    "\n",
    "print(stringtoint(\"1231920\"))"
   ]
