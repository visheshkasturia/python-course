    "def checkPalindrome(str):\n",
    "    print(str)\n",
    "    if len(str)==1:\n",
    "        return True\n",
    "    if len(str)==2:\n",
    "        return str[0]==str[-1]\n",
    "    return str[0]==str[-1] and checkPalindrome(str[1:len(str)-1])\n",
    "\n",
    "print(checkPalindrome(\"abccba\"))"
   ]
