    "def uniqueChars(string):\n",
    "    d={}\n",
    "    s=\"\"\n",
    "    for char in string:\n",
    "        if char not in d:\n",
    "                s=s+char\n",
    "        d[char]=d.get(char,0)+1\n",
    "    \n",
    "    return s\n",
    "\n",
    "# Main\n",
    "string = input()\n",
    "print(uniqueChars(string))\n"
   ]
  }
